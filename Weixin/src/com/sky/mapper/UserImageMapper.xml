<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace指用户自定义的命名空间。 -->
<mapper namespace="com.sky.mapper.UserImageMapper">
<!-- 完成最基本的增加的作用 -->
	<!-- id="save"是唯一的标示符 parameterType属性指明插入时使用的参数类型 useGeneratedKeys="true"表示使用数据库的自动增长策略 -->
	<insert id="save" parameterType="com.sky.database.UserImage"
		>
		<!-- <selectKey keyProperty="id" resultType="Java.lang.Integer" order="AFTER" ></selectKey> -->
		
		INSERT INTO userimage(fromusername,tousername,createtime,msgtype,picurl,mediaid)
		VALUES(#{FromUserName},#{ToUserName},#{CreateTime},#{MsgType},#{PicUrl},#{MediaId})
	</insert>
	<!-- 删除和修改操作可以直接去数据库查 -->
	<!-- <select id="select" parameterType="com.sky.test.User" resultType="user"> -->
	<!-- 
	<select id="select" parameterType="int" resultType="map">
		select * from
		user where id=#{id}
	</select>
		
	<delete id="delete" parameterType="int">
		delete from user where id=#{id}
	</delete>
	 -->
	

</mapper>